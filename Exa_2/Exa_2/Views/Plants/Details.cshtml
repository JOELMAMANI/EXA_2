@model IEnumerable<Exa_2.Data.Entity.Floors>

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Plant_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plant_family)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plant_Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plant_Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plant_years)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plant_cost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Planting_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Planting_amount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plant_sold)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Medicinal_plants)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Indoor_plants)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActivate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Plant_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plant_family)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plant_Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plant_Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plant_years)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plant_cost)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Planting_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Planting_amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plant_sold)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Medicinal_plants)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Indoor_plants)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActivate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
